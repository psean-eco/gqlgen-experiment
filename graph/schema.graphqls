# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Team {
  id: ID!
  name: String!
  location: String
  players: [Player!]!
  conference: String!
  abbreviation: String!
}

type Query {
  teams: [Team!]!
  players: [Player!]!
}

type Player {
  id: ID!
  name: String!
  position: String!
}
